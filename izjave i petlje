if!
let x = 10;

if (x > 5) {
    console.log("x je veći od 5");
}

if else! 

let y = 3;

if (y > 5) {
    console.log("y je veći od 5");
} else {
    console.log("y nije veći od 5");
}


for petlja!

for (let i = 0; i < 5; i++) {
    console.log(i);
}


while! petlja

let j = 0;

while (j < 5) {
    console.log(j);
    j++;
}

do while! petlja

let k = 0;

do {
    console.log(k);
    k++;
} while (k < 5);
